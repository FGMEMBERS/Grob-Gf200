<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>onoff.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>on-off</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>on-off</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>

</PropertyList>
